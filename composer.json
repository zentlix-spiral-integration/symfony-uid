{
  "name": "spiral-cms/symfony-uid",
  "type": "library",
  "description": "The package provides the ability to integrate Symfony UID into the Cycle ORM",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "cycle/entity-behavior": "^1.0",
    "doctrine/annotations": "^1.13.2",
    "doctrine/inflector": "^2.0",
    "symfony/uid": "^5.4 || ^6.0"
  },
  "require-dev": {
    "cycle/annotated": "^3.0",
    "spiral/tokenizer": "^2.10",
    "phpunit/phpunit": "^9.5.19",
    "vimeo/psalm": "^4.22"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "SpiralCMS\\SymfonyUID\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test" : "./vendor/bin/simple-phpunit"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
